            "schema": {
              "anyOf": [
                {
                  "type": "number"
                },
                {
                  "type": "null"
                }
              ],
              "title": "Ac Power Measurement"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
